{"code":"import { Hint } from \"./hint\";\nimport { CurrencyID } from \"./id\";\nimport { HINT } from \"../alias\";\nimport { Assert, ECODE, MitumError } from \"../error\";\nimport { Big } from \"../types\";\nexport class Amount {\n    constructor(currency, big) {\n        this.hint = new Hint(HINT.CURRENCY.AMOUNT);\n        this.currency = CurrencyID.from(currency);\n        this.big = Big.from(big);\n        Assert.check(0 < this.big.big, MitumError.detail(ECODE.INVALID_AMOUNT, \"zero big\"));\n    }\n    toBuffer() {\n        return Buffer.concat([\n            this.big.toBuffer(),\n            this.currency.toBuffer(),\n        ]);\n    }\n    toHintedObject() {\n        return {\n            _hint: this.hint.toString(),\n            currency: this.currency.toString(),\n            amount: this.big.toString(),\n        };\n    }\n}\n//# sourceMappingURL=amount.js.map","references":["/Users/ohayoung/Projects/npm/mitumjs/src/common/hint.ts","/Users/ohayoung/Projects/npm/mitumjs/src/common/id.ts","/Users/ohayoung/Projects/npm/mitumjs/src/alias/index.ts","/Users/ohayoung/Projects/npm/mitumjs/src/error/index.ts","/Users/ohayoung/Projects/npm/mitumjs/src/types/index.ts"],"map":"{\"version\":3,\"file\":\"amount.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../../src/common/amount.ts\"],\"names\":[],\"mappings\":\"AAAA,OAAO,EAAE,IAAI,EAAE,MAAM,QAAQ,CAAA;AAC7B,OAAO,EAAE,UAAU,EAAE,MAAM,MAAM,CAAA;AAEjC,OAAO,EAAE,IAAI,EAAE,MAAM,UAAU,CAAA;AAC/B,OAAO,EAAE,MAAM,EAAE,KAAK,EAAE,UAAU,EAAE,MAAM,UAAU,CAAA;AACpD,OAAO,EAAE,GAAG,EAAwC,MAAM,UAAU,CAAA;AAEpE,MAAM,OAAO,MAAM;IAKf,YAAY,QAA6B,EAAE,GAA0B;QACjE,IAAI,CAAC,IAAI,GAAG,IAAI,IAAI,CAAC,IAAI,CAAC,QAAQ,CAAC,MAAM,CAAC,CAAA;QAC1C,IAAI,CAAC,QAAQ,GAAG,UAAU,CAAC,IAAI,CAAC,QAAQ,CAAC,CAAA;QACzC,IAAI,CAAC,GAAG,GAAG,GAAG,CAAC,IAAI,CAAC,GAAG,CAAC,CAAA;QACxB,MAAM,CAAC,KAAK,CAAC,CAAC,GAAG,IAAI,CAAC,GAAG,CAAC,GAAG,EAAE,UAAU,CAAC,MAAM,CAAC,KAAK,CAAC,cAAc,EAAE,UAAU,CAAC,CAAC,CAAA;IACvF,CAAC;IAED,QAAQ;QACJ,OAAO,MAAM,CAAC,MAAM,CAAC;YACjB,IAAI,CAAC,G